/* pages/payment/commonUse/index/index.wxss */
.card{
  padding:50rpx 30rpx;
  line-height: 56rpx;
}
/*分割线样式*/
.divLine{
  background: #E0E3DA;
  width: 100%;
  height: 3rpx;
  margin: 10rpx 0;
} 
.my-pay{
  margin:30rpx 20rpx 0 20rpx ;
  font-weight: bold;
}
.arrow{
  width: 10px;
  height: 10px; 
  border-top: 2px solid #999;
  border-right: 2px solid #999; 
  position: absolute; 
  right: 50rpx;
  transform: rotate(45deg);
}
.container {
  padding-bottom: 120rpx;
}